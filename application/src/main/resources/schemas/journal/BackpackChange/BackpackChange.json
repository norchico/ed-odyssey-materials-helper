{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "This is written when there is any change to the contents of the suit backpack â€“ note this can be written at the same time as other events like UseConsumable",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "Added" : {
      "title" : "Added",
      "type" : "array",
      "default": null,
      "items" : {
        "$ref": "#/definitions/changeEntry"
      }
    },
    "Removed" : {
      "title" : "Removed",
      "type" : "array",
