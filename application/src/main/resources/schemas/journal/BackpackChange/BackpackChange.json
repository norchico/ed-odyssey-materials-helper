        "OwnerID" : {
          "title" : "OwnerID",
          "type" : "integer",
          "examples" : [ 0, 2954264951, 2952913247 ]
        },
        "Count" : {
          "title" : "Count",
          "type" : "integer",
          "examples" : [ 1, 8, 4 ]
        },
        "Type" : {
          "title" : "Type",
          "type" : "string",
          "examples" : [ "Consumable", "Component", "Item" ]
        },
        "MissionID" : {
          "title" : "MissionID",
          "type" : "integer",
          "examples" : [ 839014036, 895446120, 895536604 ]
