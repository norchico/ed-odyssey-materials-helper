{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "When written: when a ship's fighter is rebuilt in the hangar",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "Loadout" : {
      "title" : "Loadout",
      "type" : "string",
      "examples" : [ "zero", "one" ]
    },
    "ID" : {
      "title" : "ID",
      "type" : "integer",
      "examples" : [ 68, 69, 52 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "Loadout", "ID" ]
}