{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "javaType" : "nl.jixxed.eliteodysseymaterials.schemas.journal.Event",
  "title": "Event",
  "description": "Generic Event",
  "properties" : {
    "timestamp" : {
      "title" : "timestamp",
      "description" : "Timestamp in UTC, ISO 8601",
      "type" : "string",
      "format" : "date-time",
      "examples" : [ "2022-09-11T22:00:45Z", "2022-09-12T08:54:21Z", "2022-09-12T08:54:24Z" ]
    },
    "event" : {
      "title" : "event",
      "type" : "string",
      "examples" : [ "AfmuRepairs", "FSDJump", "Location" ]
    }
  },
  "additionalProperties": false,
  "required" : [ "timestamp", "event" ]
}