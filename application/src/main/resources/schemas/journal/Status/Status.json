{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "description": "This event is written to Status.json which is updated every few seconds",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "Flags" : {
      "title" : "Flags",
      "type" : "integer",
      "examples" : [ 16842765, 18874376, 0 ]
    },
    "Pips" : {
      "title" : "Pips",
      "type" : "array",
      "default": null,
      "items" : {
        "type" : "integer",
        "examples" : [ 2, 8, 4 ]
      }
    },
    "FireGroup" : {
      "title" : "FireGroup",
      "type" : "integer",
      "examples" : [ 0, 1, 3 ]
    },
    "Fuel" : {
      "title" : "Fuel",
      "type" : "object",
      "properties" : {
        "FuelMain" : {
          "title" : "FuelMain",
          "type" : "number",
          "examples" : [ 15.146626, 8.0, 0.0 ]
        },
        "FuelReservoir" : {
          "title" : "FuelReservoir",
          "type" : "number",
          "examples" : [ 0.382796, 0.41, 0.404189 ]
        }
      },
      "additionalProperties" : false,
      "required" : [ "FuelMain", "FuelReservoir" ]
    },
    "GuiFocus" : {
      "title" : "GuiFocus",
      "type" : "integer",
      "examples" : [ 5, 0, 2 ]
    },
    "Latitude" : {
      "title" : "Latitude",
      "type" : "number",
      "examples" : [ -28.584963, -66.410538, -66.413887 ]
    },
    "Longitude" : {
      "title" : "Longitude",
      "type" : "number",
      "examples" : [ 6.826313, 19.822344, 19.815832 ]
    },
    "Heading" : {
      "title" : "Heading",
      "type" : "integer",
      "examples" : [ 109, 34, 37 ]
    },
    "Altitude" : {
      "title" : "Altitude",
      "type" : "integer",
      "examples" : [ 404, 0, 88 ]
    },
    "Flags2" : {
      "title" : "Flags2",
      "type" : "integer",
      "examples" : [ 0, 90121, 35345 ]
    },
    "Cargo" : {
      "title" : "Cargo",
      "type" : "number",
      "examples" : [ 0.0 ]
    },
    "LegalState" : {
      "title" : "LegalState",
      "type" : "string",
      "examples" : [ "Clean", "Speeding" ]
    },
    "Balance" : {
      "title" : "Balance",
      "type" : "integer",
      "examples" : [ 1593446845, 1593431575 ]
    },
    "Oxygen" : {
      "title" : "Oxygen",
      "type" : "number",
      "examples" : [ 1.0 ]
    },
    "Health" : {
      "title" : "Health",
      "type" : "number",
      "examples" : [ 1.0 ]
    },
    "Temperature" : {
      "title" : "Temperature",
      "type" : "number",
      "examples" : [ 293.0, 713.976318, 713.944336 ]
    },
    "SelectedWeapon" : {
      "title" : "SelectedWeapon",
      "type" : "string",
      "examples" : [ "", "$humanoid_fists_name;", "$wpn_m_sniper_plasma_charged_name;" ]
    },
    "BodyName" : {
      "title" : "BodyName",
      "type" : "string",
      "examples" : [ "Crown Depot", "Harow A 1" ]
    },
    "Destination" : {
      "title" : "Destination",
      "type" : "object",
      "properties" : {
        "System" : {
          "title" : "System",
          "type" : "integer",
          "examples" : [ 7269098333561 ]
        },
        "Body" : {
          "title" : "Body",
          "type" : "integer",
          "examples" : [ 5 ]
        },
        "Name" : {
          "title" : "Name",
          "type" : "string",
          "examples" : [ "Harow" ]
        },
        "Name_Localised" : {
          "title" : "Name",
          "type" : "string",
          "examples" : [ "Harow" ]
        }
      },
      "additionalProperties" : false,
      "required" : [ "System", "Body", "Name" ]
    },
    "PlanetRadius" : {
      "title" : "PlanetRadius",
      "type" : "number",
      "examples" : [ 2321861.5 ]
    },
    "SelectedWeapon_Localised" : {
      "title" : "SelectedWeapon_Localised",
      "type" : "string",
      "examples" : [ "Unarmed", "Manticore Executioner", "Manticore Tormentor" ]
    },
    "Gravity" : {
      "title" : "Gravity",
      "type" : "number",
      "examples" : [ 0.347632 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "Flags" ]
}