{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "extends" : { "$ref": "../_Event.json"},
  "allOf": [{ "$ref": "../_Event.json"}],
  "properties" : {
    "SquadronName" : {
      "title" : "SquadronName",
      "type" : "string",
      "examples" : [ "TestSquadron" ]
    },
    "OldRank" : {
      "title" : "OldRank",
      "type" : "integer",
      "examples" : [ 3 ]
    },
    "NewRank" : {
      "title" : "NewRank",
      "type" : "integer",
      "examples" : [ 2 ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "SquadronName", "OldRank", "NewRank" ]
}